# Environment Configuration for {{ project_name }}
# Copy this file to .env.local and fill in your values

# Development Configuration
VITE_APP_NAME="{{ project_name }}"
VITE_PORT={{ port_number }}

# API Configuration{% if api_key %}
VITE_API_KEY="{{ api_key }}"{% else %}
# VITE_API_KEY=your_api_key_here{% endif %}

# Deployment Target
DEPLOYMENT_TARGET="{{ deployment_target }}"

# Additional Features{% for feature in additional_features %}
FEATURE_{{ feature|upper }}=true{% endfor %}

# Styling Framework
STYLING_FRAMEWORK="{{ styling_framework }}"
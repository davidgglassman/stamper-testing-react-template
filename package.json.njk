{
  "name": "{{ project_name }}",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": "{{ author_name }}",
  "scripts": {
    "dev": "vite --port {{ port_number }}",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"{% if include_testing %},
    "test": "jest",
    "test:watch": "jest --watch"{% endif %}
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"{% if styling_framework == "tailwindcss" %},
    "tailwindcss": "^3.4.1",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35"{% endif %}{% if styling_framework == "styled-components" %},
    "styled-components": "^6.1.8"{% endif %}{% if styling_framework == "emotion" %},
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0"{% endif %}{% if "routing" in additional_features %},
    "react-router-dom": "^6.21.3"{% endif %}{% if "state-management" in additional_features %},
    "zustand": "^4.5.0"{% endif %}{% if "i18n" in additional_features %},
    "react-i18next": "^14.0.5",
    "i18next": "^23.8.2"{% endif %}{% if "pwa" in additional_features %},
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0"{% endif %}
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",{% if include_testing %}
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",{% endif %}
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  }
}
